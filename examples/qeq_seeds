inv(R)
inv(add(R, div(2.0, _sym_add_B))
div(1.2, add(R, div(2.4, _sym_add_B))
inv(sqrt(add(square(R), square(div(2.0, _sym_add_B)))))
inv(sqrt(add(square(R)), square(mul(0.5, _sym_inv_add_B))))
inv(add(R, mul(0.5, mul(exp(mul(0.4, R)), _sym_add_B))))
inv(cbrt(add(cube(R), cube(div(2.0, _sym_add_B)))))
