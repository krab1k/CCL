div(1.2, add(R, div(2.4, add(B(i), B(j)))
div(1.0, sqrt(add(square(R), square(div(2.0, add(B(i), B(j)))))))
div(1.0, sqrt(add(square(R)), square(add(div(0.5, B(i)), div(0.5, B(j)))))))
